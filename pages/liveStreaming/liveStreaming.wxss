/* pages/liveStreaming/liveStreaming.wxss */

.page-body {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  color: #fbfbfb;
  font-size: 24rpx;
}

.live-player {
  width: 100%;
  z-index: -1;
  /* background: #6666; */
}

.top {
  width: 100%;
  display: flex;
  flex-direction: row;
  position: fixed;
  top: 0;
  z-index: 999;
  margin-top: 35rpx;
}

.head-image {
  width: 82rpx;
  height: 82rpx;
  margin-left: 24rpx;
  border-radius: 50%;
  background: #fff;
}

.top-content {
  margin-left: 16rpx;
  display: flex;
  flex-direction: column;
}

.content-title {
  display: flex;
  flex-direction: row;
  text-align: center;
  align-items: center;
}

.title-view {
  width: 1rpx;
  height: 32rpx;
  background: #fff;
  margin-left: 24rpx;
  margin-right: 24rpx;
}

.content-number {
  margin-top: 8rpx;
}

.bottome {
  display: flex;
  flex-direction: row;
  position: fixed;
  z-index: 999;
  bottom: 0;
  margin-bottom: 24rpx;
  text-align: center;
  align-items: center;
}

.zb-cart {
  width: 75rpx;
  height: 82rpx;
  margin-left: 30rpx;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}

.bottom-input {
  width: 370rpx;
  height: 64rpx;
  background-color: #fff;
  margin-left: 30rpx;
  border-radius: 8px;
  opacity: 0.7489;
  color: #000;
}

.zb-share {
  width: 68rpx;
  height: 50rpx;
  margin-left: 24rpx;
}

.zb-collect {
  width: 68rpx;
  height: 63rpx;
  margin-left: 48rpx;
}

.zb-content {
  display: flex;
  flex-direction: column;
  height: 184rpx;
  margin-left: 24rpx;
  position: fixed;
  bottom: 0;
  z-index: 999;
  margin-bottom: 121rpx;
  color: #fff;
}

.zb-content-text {
  word-break: break-all;
}

.zb-content-title {
  width: 140rpx;
  height: 36rpx;
  border-radius: 16rpx;
  background: #e83d2c;
  text-align: center;
  font-size: 24rpx;
  margin-bottom: 5rpx;
}

/*使屏幕变暗  */

.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

/*对话框 */

.commodity_attr_box {
  width: 702rpx;
  max-height: 862rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  padding: 20rpx 3%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  margin-left: 24rpx;
  border-radius: 16rpx 16rpx 0rpx 0rpx;
}

.shop-item {
  display: flex;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
}

.shop-item-icon {
  width: 144rpx;
  height: 144rpx;
}

.shop-item-content {
  display: flex;
  flex-direction: column;
  width: 410rpx;
  margin-left: 24rpx;
}

.item-content-name {
  font-size: 24rpx;
  color: #313131;
}

.item-content-price {
  font-size: 24rpx;
  color: #e83d2c;
  margin-top: 12rpx;
}

.item-content-cart {
  width: 52rpx;
  height: 48rpx;
  margin-left: 24rpx;
}

.line {
  margin-top: 24rpx;
  width: 654rpx;
  height: 2rpx;
  background: rgba(193, 193, 193, 1);
}
